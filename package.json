{
	"name": "kigaluxe",
	"version": "1.0.0",
	"main": "src/index.js",
	"type": "commonjs",
	"scripts": {
		"test": "nyc --reporter=text --reporter=lcov jest",
		"build": "rimraf dist && babel src --out-dir dist  --copy-files",
		"start": "npm run  && npm run build && node dist/",
		"dev": "nodemon --exec babel-node src/index.js",
		"lint": "eslint ./src",
		"coverage": "nyc npm run test",
		"db:create": "npx sequelize-cli db:create",
		"drop_migrate_seed": "npm run droptables && npm run migrate && npm run seed",
		"migrate": "npx sequelize-cli db:migrate",
		"seed": "npx sequelize-cli db:seed:all",
		"droptables": "npx sequelize-cli db:migrate:undo:all",
		"db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
		"migrate:test": "cross-env NODE_ENV=test npx sequelize-cli db:migrate",
		"seed:test": "cross-env NODE_ENV=test npx sequelize-cli db:seed:all",
		"droptables:test": "cross-env NODE_ENV=test npx sequelize-cli db:migrate:undo:all",
		"db:create:pro": "cross-env NODE_ENV=production npx sequelize-cli db:create",
		"migrate:pro": "cross-env NODE_ENV=production npx sequelize-cli db:migrate",
		"seed:pro": "cross-env NODE_ENV=production npx sequelize-cli db:seed:all",
		"droptables:pro": "cross-env NODE_ENV=production npx sequelize-cli db:migrate:undo:all"
	},
	"author": "Igor J.L. Ndiramiye",
	"license": "ISC",
	"description": "",
	"devDependencies": {
		"@babel/cli": "^7.24.8",
		"@babel/core": "^7.24.9",
		"@babel/node": "^7.24.8",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-object-rest-spread": "^7.20.7",
		"@babel/polyfill": "^7.12.1",
		"@babel/preset-env": "^7.24.8",
		"@babel/register": "^7.24.6",
		"chai": "^5.1.1",
		"chai-http": "^5.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.53.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import": "^2.29.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"esm": "^3.2.25",
		"jest": "^29.7.0",
		"morgan": "^1.10.0",
		"nodemon": "^3.1.4",
		"nyc": "^17.0.0",
		"rimraf": "^6.0.1",
		"sequelize-cli": "^6.6.2",
		"supertest": "^7.0.0"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.623.0",
		"@aws-sdk/s3-request-presigner": "^3.623.0",
		"@google/generative-ai": "^0.21.0",
		"aws-sdk": "^2.1667.0",
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"express-session": "^1.18.0",
		"fs": "^0.0.1-security",
